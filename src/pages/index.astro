---
import Layout from '../layouts/Layout.astro';
import Navbar from '../components/Navbar.astro'
import HeroSection from '../components/HeroSection.astro'
import ModelY from '../components/ModelY.astro'
import Model3 from '../components/Model3.astro'
import ModelX from '../components/ModelX.astro'
import ModelS from '../components/ModelS.astro'
import ModelNone from '../components/ModelNone.astro'
import SolarPanels from '../components/SolarPanels.astro'
import SolarRoof from '../components/SolarRoof.astro'
import Powerfall from '../components/Powerfall.astro'
import Accessories from '../components/Accessories.astro'
---

<Layout title="Tesla Clon">
		<Navbar/>
	<!-- Background -->
	<main class="relative w-full h-screen overflow-auto snap-y snap-mandatory">
		<div class="snap-center">
			<HeroSection/>
		</div>
		<div class="snap-center">
			<ModelY/>
		</div>
		<div class="snap-center">
			<Model3/>
		</div>
		<div class="snap-center">
			<ModelX/>
		</div>
		<div class="snap-center">
			<ModelS/>
		</div>
		<div class="snap-center">
			<ModelNone/>
		</div>
		<div class="snap-center">
			<SolarPanels/>
		</div>
		<div class="snap-center">
			<SolarRoof/>
		</div>
		<div class="snap-center">
			<Powerfall/>
		</div>
		<div class="snap-center">
			<Accessories/>
		</div>
		
	</main>
</Layout>

